{
  "name": "commongateway/corebundle",
  "type": "symfony-bundle",
  "description": "Symfony Bundle for dutch common ground functionality",
  "keywords": [
    "commongateway",
    "common",
    "gateway",
    "conduction",
    "symfony"
  ],
  "homepage": "https://commongatway.nl",
  "license": "MIT",
  "authors": [
    {
      "name": "Ruben van der Linde",
      "email": "ruben@conduction.nl"
    },
    {
      "name": "Conduction",
      "homepage": "https://conduction.nl"
    }
  ],
  "require": {
    "php": "^8.2.0",
    "symfony/monolog-bundle": "^3.8",
    "ext-ctype": "*",
    "ext-fileinfo": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "adbario/php-dot-notation": "^3",
    "alcaeus/mongo-php-adapter": "^1.2",
    "api-platform/core": "^2.6 | ^3.1",
    "composer/package-versions-deprecated": "1.11.99.3",
    "doctrine/annotations": "^1.14 | ^2.0",
    "doctrine/doctrine-bundle": "^2.7",
    "doctrine/doctrine-migrations-bundle": "^3.1",
    "doctrine/orm": "^2.14",
    "dompdf/dompdf": "^2",
    "endroid/qr-code-bundle": "^3.4 | ^4.2",
    "guzzlehttp/guzzle": "^6.5.8 | ^7.5",
    "guzzlehttp/psr7": "1.9 | ^2.4",
    "hautelook/alice-bundle": "^2.9.0",
    "jwadhams/json-logic-php": "^1.4",
    "league/flysystem-bundle": "^2.4",
    "league/flysystem-ftp": "^2.4",
    "league/flysystem-ziparchive": "^2.3",
    "mongodb/mongodb": "^1.15",
    "monolog/monolog": "^2.8.0",
    "nelmio/cors-bundle": "^2.1",
    "phpdocumentor/reflection-docblock": "^5.2",
    "phpoffice/phpword": "^0.18.2",
    "phpoffice/phpspreadsheet": "^1.29",
    "respect/validation": "^2.2",
    "setono/cron-expression-bundle": "^1.5",
    "symfony/asset": "^5.4 | ^6.4",
    "symfony/console": "^5.4 | ^6.4",
    "symfony/contracts": "^2.4.0 | ^3.1",
    "symfony/dotenv": "^5.4 | ^6.4",
    "symfony/expression-language": "^5.4 | ^6.4",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "^5.4 | ^6.4",
    "symfony/http-client": "^5.4 | ^6.4",
    "symfony/mailer": "^5.4 | ^6.4",
    "symfony/mailgun-mailer": "^5.4 | ^6.4",
    "symfony/messenger": "^5.4 | ^6.4",
    "symfony/property-access": "^5.4 | ^6.4",
    "symfony/property-info": "^5.4 | ^6.4",
    "symfony/proxy-manager-bridge": "^5.4 | ^6.4",
    "symfony/runtime": "^5.4 | ^6.4",
    "symfony/security-bundle": "^5.4 | ^6.4",
    "symfony/serializer": "^5.4 | ^6.4",
    "symfony/twig-bundle": "^5.4 | ^6.4",
    "symfony/validator": "^5.4 | ^6.4",
    "symfony/web-profiler-bundle": "^5.4 | ^6.4",
    "symfony/yaml": "^5.4 | ^6.4"
  },
  "require-dev": {
    "cache/integration-tests": "dev-master",
    "doctrine/cache": "~1.6",
    "doctrine/dbal": "~2.5",
    "predis/predis": "~1.1",
    "symfony/config": "^5.1",
    "symfony/dependency-injection": "~3.4|~4.1|~5.0",
    "symfony/var-dumper": "^4.1.1|^5.1",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "^5.4 | ^6.4",
    "symfony/css-selector": "^5.4 | ^6.4",
    "symfony/phpunit-bridge": "^6.1",
    "symfony/stopwatch": "^5.4 | ^6.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true,
      "endroid/installer": true
    }
  },
  "conflict": {
    "doctrine/dbal": "<2.5",
    "symfony/dependency-injection": "<3.4",
    "symfony/var-dumper": "<3.4"
  },
  "autoload": {
    "psr-4": {
      "CommonGateway\\CoreBundle\\": "src"
    }
  },
  "minimum-stability": "alpha",
  "support": {
    "email": "info@conduction.nl",
    "issues": "https://github.com/CommonGateway/CoreBundle/issues",
    "source": "https://github.com/CommonGateway/CoreBundle"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/CommonGateway/CoreBundle",
      "name": "source"
    }
  ]
}